<?xml version="1.0" encoding="utf-8" ?>
<mappings>
  <!-- The root package path is created with -->
  <!-- the path passed into to the           -->
  <!-- xbdeploy.exe pull command.           -->
  <path source="%Q3INSTALLDIR%\baseq3"
        target="\baseq3" />
  <path source="%Q3INSTALLDIR%\missionpack"
        target="\missionpack" />
  <path source="[CodeDir]\..\baseq3"
        target="\baseq3" />
  <path source="[CodeDir]\..\missionpack"
        target="\missionpack" />
  <path source="[OutDir]\baseq3"
        target="\baseq3" />
  <path source="[OutDir]\missionpack"
        target="\missionpack" />
</mappings>
